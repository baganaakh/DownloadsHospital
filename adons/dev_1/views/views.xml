<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="dev_1.list">
      <field name="name">dev_1 list</field>
      <field name="model">dev_1.dev_1</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="plate_number"/>
          <field name="enter_date"/>
          <field name="out_date"/>
          <field name="status"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="dev_1.action_window">
      <field name="name">dev_1 window</field>
      <field name="res_model">dev_1.dev_1</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="dev_1.action_server">
      <field name="name">dev_1 server</field>
      <field name="model_id" ref="model_dev_1_dev_1"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="dev_1" id="dev_1.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="dev_1.menu_1" parent="dev_1.menu_root"/>
    <menuitem name="Menu 2" id="dev_1.menu_2" parent="dev_1.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="dev_1.menu_1_list" parent="dev_1.menu_1"
              action="dev_1.action_window"/>
    <menuitem name="Server to list" id="dev_1" parent="dev_1.menu_2"
              action="dev_1.action_server"/>

  </data>
</odoo>